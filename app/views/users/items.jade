#items
    h3 Purchases
    each item in user.items
        case item.type
            when 'autogrow'
                include ../items/autogrow
            when 'autoseed'
                include ../items/autoseed
            when 'autoroot'
                include ../items/autoroot
            when 'basichouse'
                include ../items/basichouse
            when 'mansion'
                include ../items/mansion
            when 'castle'
                include ../items/castle

